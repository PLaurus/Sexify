CREATE TABLE IF NOT EXISTS TaskDoerSex (
	id INTEGER NOT NULL PRIMARY KEY,
	taskId INTEGER NOT NULL,
	sexId INTEGER NOT NULL,
	FOREIGN KEY (id)
		REFERENCES Tasks(id)
			ON UPDATE CASCADE
			ON DELETE CASCADE,
	FOREIGN KEY (sexId)
        REFERENCES Sex(id)
            ON UPDATE CASCADE
            ON DELETE CASCADE,
    UNIQUE (taskId, sexId)
);