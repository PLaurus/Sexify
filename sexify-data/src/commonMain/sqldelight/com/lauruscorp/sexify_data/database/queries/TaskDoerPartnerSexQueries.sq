selectTaskDoerPatnerSexesByTaskId:
SELECT *
FROM TaskDoerPartnerSex
GROUP BY taskId, sexId
HAVING taskId = :taskId;

insertTaskDoerPatnerSex:
INSERT OR IGNORE INTO TaskDoerPartnerSex
VALUES ?;

insertTaskDoerPatnerSexFields:
INSERT OR IGNORE INTO TaskDoerPartnerSex(taskId, sexId)
VALUES (:taskId, :sexId);

deleteTaskDoerPatnerSex:
DELETE FROM TaskDoerPartnerSex
WHERE taskId = :taskId AND sexId = :sexId;